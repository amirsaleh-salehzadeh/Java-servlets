<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>RegServlet</servlet-name>
        <servlet-class>RegServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logIn</servlet-name>
        <servlet-class>logIn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>LogOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertMovie</servlet-name>
        <servlet-class>InsertMovie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fetchAllMovies</servlet-name>
        <servlet-class>fetchAllMovies</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TicketReserved</servlet-name>
        <servlet-class>TicketReserved</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>myPage</servlet-name>
        <servlet-class>myPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateUser</servlet-name>
        <servlet-class>updateUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegServlet</servlet-name>
        <url-pattern>/RegServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logIn</servlet-name>
        <url-pattern>/logIn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/LogOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertMovie</servlet-name>
        <url-pattern>/InsertMovie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fetchAllMovies</servlet-name>
        <url-pattern>/fetchAllMovies</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TicketReserved</servlet-name>
        <url-pattern>/TicketReserved</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>myPage</servlet-name>
        <url-pattern>/myPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateUser</servlet-name>
        <url-pattern>/updateUser</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        </welcome-file-list>
    </web-app>
